<h2>Projects</h2>
<table class="url-list sortable">
	<tr>
		<th>Name</th>
		<th>Created</th>
		<th>Updated</th>
		<th>Status</th>
		<th>Company</th>
		<th></th>
	</tr>
<%= render @projects %>
</table>
<%= button_to '(Re-)Import All from basecamp', import_projects_path, :class => "inline", :confirm => "Are you sure?  All project data will be deleted first..." %>
<%= button_to 'Load new projects from basecamp', discover_projects_path, :class => "inline" %>
<%= button_to 'Create new project', new_project_path,  :method => 'get'%>

